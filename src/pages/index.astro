  <body class="bg-sky-100 dark:bg-sky-950 dark:text-white">
    <nav class="flex justify-center z-0">
      <Navbar />
    </nav>

    <div class="p-10"></div>
    <div class="relative w-full md:px-5 overflow-x-clip flex justify-center">
      <div class="relative w-5/6 max-w-6xl">
        <div
          class="rounded-full bg-green-300 h-96 w-96 absolute top-24 -left-5 -z-10 filter mix-blend-multiply blur-2xl animate-blob"
